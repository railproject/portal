<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd"
		default-lazy-init="true">

	<description>引入properties配置文件</description>

    <!-- 引入配置文件 -->
    <util:properties id="restConfig" location="classpath:config/restConfig.properties"/>

    <util:properties id="rabbitmqConfig" location="classpath:config/rabbitmqConfig.properties"/>
</beans>